<%= form_for @achievement do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <%= f.label :team %>
  <%= f.text_field :team%>
  <%= f.label :headline %>
  <%= f.text_field :headline%>
  <%= f.label :description %>
  <%= f.text_field :description%>
  <%= f.label :categories %>
  <%= f.text_field :categories%>
  <%= f.label :date %>
  <%= f.date_field :date%>
  <%= f.submit "Submit Achievement"%>
<% end %>
